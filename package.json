{
  "name": "crosscourt-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "commit": "git-cz",
    "eject": "react-scripts eject",
    "format": "prettier --write '{src,__{tests,mocks}__}/**/*.{js,jsx,json,scss}'",
    "lint": "npm-run-all lint:code",
    "lint:code": "eslint -c .eslintrc src --color",
    "lint:fix": "eslint --fix src",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,scss,css}": [
      "prettier --write",
      "yarn lint:fix --quiet",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@mars/heroku-js-runtime-env": "^3.0.2",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "cheeseburger-menu": "^0.0.13",
    "connected-react-router": "^6.5.2",
    "currency.js": "^1.2.2",
    "dayjs": "^1.8.19",
    "formik": "^2.0.3",
    "google-map-react": "^1.1.5",
    "history": "^4.10.1",
    "humps": "^2.0.1",
    "node-sass": "^4.13.0",
    "polished": "^3.4.2",
    "query-string": "^6.8.3",
    "ramda": "^0.26.1",
    "react": "^16.11.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.11.0",
    "react-ga": "^2.7.0",
    "react-hamburger-menu": "^1.1.1",
    "react-modal": "^3.11.1",
    "react-player": "^1.15.3",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-scrolllock": "^4.0.1",
    "react-select": "^3.0.8",
    "react-share": "^4.0.1",
    "react-spinners": "^0.6.1",
    "react-sticky": "^6.0.3",
    "react-stripe-elements": "^6.0.1",
    "react-stripe-script-loader": "^1.1.0",
    "react-table": "^7.0.0-rc.16",
    "react-toastify": "^5.5.0",
    "react-tooltip": "^4.0.3",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.1",
    "reselect": "^4.0.0",
    "styled-components": "^4.4.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "husky": "^2.4.0",
    "lint-staged": "^8.2.0",
    "npm-run-all": "4.1.3",
    "prettier": "^1.18.2"
  }
}
