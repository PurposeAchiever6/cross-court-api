                  <tr>
                    <td class="pad">
                      <div style="color:#101112;direction:ltr;font-family:Verdana, Geneva, sans-serif;font-size:16px;font-weight:400;letter-spacing:0px;line-height:120%;text-align:left;mso-line-height-alt:19.2px;">
                        <p style="margin: 0;">
                          <strong>
                            <%= user_session.date.strftime(Session::MONTH_NAME_FORMAT) %>
                          </strong>
                        </p>
                      </div>
                    </td>
                  </tr>
                </table>
                <table border="0" cellpadding="10" cellspacing="0" class="paragraph_block block-2" role="presentation" style="mso-table-lspace: 0pt; mso-table-rspace: 0pt; word-break: break-word;" width="100%">
