                        <p style="margin: 0;">
                          <%= user_session.time.strftime(Session::TIME_FORMAT).upcase %> - <%= (user_session.time + session.duration_minutes.minutes).strftime(Session::TIME_FORMAT).upcase %>
                        </p>
                      </div>
                    </td>
                  </tr>
                </table>
              </td>
            </tr>
          </tbody>
        </table>
      </td>
    </tr>
  </tbody>
</table>
